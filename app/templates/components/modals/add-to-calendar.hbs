<i class="black close icon"></i>
<div class="header">
    {{t 'Please choose a calender'}}
</div>
<div class="ui content">
    {{#each this.calenders as |calender|}}
        <div class="ui link list">
            <div class="item">
                <i class="ui {{calender.icon}} {{calender.color}} icon"></i>
                <div class="content">
                    <div class="header">
                        <a href={{calender.url}} class="item active" target="_blank" rel="noreferrer nofollow">
                            {{calender.name}}
                        </a>
                    </div>
                </div>
            </div>
        </div>
    {{/each}}
</div>
